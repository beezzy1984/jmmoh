        </record>
        <!--Sagicore Personal Individual Insurance Plan-->
         <record id="personal_individual_insurance_plan_sagicore" model="gnuhealth.insurance.plan">
            <field name="company" ref="party_sagicore" />
            <field name="name" ref="product_variant_individual_personal" />
        </record>
        <!--Sagicore Personal Family Insurance Plan-->
         <record id="personal_family_insurance_plan_sagicore" model="gnuhealth.insurance.plan">
            <field name="company" ref="party_sagicore" />
            <field name="name" ref="product_variant_individual_family" />
        </record>
        <!--Sagicore Personal Other Insurance Plan-->
         <record id="personal_other_insurance_plan_sagicore" model="gnuhealth.insurance.plan">
            <field name="company" ref="party_sagicore" />
            <field name="name" ref="product_variant_individual_other" />
        </record>
    </data>
</tryton>
